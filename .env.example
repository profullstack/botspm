# API Keys
OPENAI_API_KEY=your_openai_api_key_here
TWO_CAPTCHA_API_KEY=your_2captcha_api_key_here

# Development Settings
NODE_ENV=development
DEBUG=app:*

# Platform Credentials (for testing)
# TikTok
TIKTOK_USERNAME=your_tiktok_username
TIKTOK_PASSWORD=your_tiktok_password
TIKTOK_STREAM_KEY=your_tiktok_stream_key

# YouTube
YOUTUBE_EMAIL=your_youtube_email
YOUTUBE_PASSWORD=your_youtube_password
YOUTUBE_STREAM_KEY=your_youtube_stream_key

# X.com (Twitter)
XCOM_USERNAME=your_xcom_username
XCOM_PASSWORD=your_xcom_password
XCOM_STREAM_KEY=your_xcom_stream_key

# Database Configuration
DB_PATH=user-data/bots.sqlite

# Logging
LOG_LEVEL=info
LOG_FILE=user-data/logs/app.log

# FFmpeg Path (optional, uses system FFmpeg by default)
FFMPEG_PATH=/path/to/ffmpeg

# Chrome/Chromium Path (optional, auto-detected by default)
CHROME_PATH=/path/to/chrome

# UI Settings
DEFAULT_DARK_MODE=true